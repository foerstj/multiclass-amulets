[t:template,n:multiclass_amulet_mr_uber10]
{
	specializes = base_multiclass_amulet;

	common:screen_name	= "Melee + Ranged, Uber Level 10";
    aspect:gold_value = 60;

	[gui]
	{
		equip_requirements = melee:7,ranged:7,strength:14,dexterity:14;
	}
	[magic]
	{
		[enchantments]
		{
			[*]
			{
				alteration			= alter_strength;
				value				= 0.03;
				description			= "+0.03 to Strength";
				duration			= #infinite;
				is_permanent		= true;
				is_single_instance	= false;
			}
		}
	}
}
